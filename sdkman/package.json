{
  "name": "@sebastien-perpignane/setup-sdkman",
  "version": "0.0.0",
  "private": false,
  "description": "setup SDKMAN! in GitHub workflows",
  "main": "lib/sdkman.js",
  "types": "lib/sdkman.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build src/sdkman.ts --source-map --license licenses.txt",
    "test": "jest '^.*\\.test\\.ts$'",
    "int-test": "jest '^.*\\.int-test\\.ts$'",
    "clean": "rm -Rf lib dist",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm run test && npm run int-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastien-perpignane/actions.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "SÃ©bastien Perpignane",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.16.3",
    "@typescript-eslint/parser": "^5.59.2",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.39.0",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
